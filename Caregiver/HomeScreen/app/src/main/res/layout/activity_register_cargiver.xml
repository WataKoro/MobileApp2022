<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/wavebg"
    tools:context=".register_caregiver">


    <TextView
        android:id="@+id/sapa"
        android:layout_width="153dp"
        android:layout_height="46dp"

        android:fontFamily="@font/poppins_medium"
        android:text="@string/data_diri"
        android:textAlignment="center"
        android:textColor="@color/design_default_color_surface"
        android:textColorHighlight="#FFFFFF"
        android:textColorLink="@color/white"
        android:textSize="29sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.023"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/Nama"
        android:layout_width="303dp"
        android:layout_height="40dp"
        android:layout_marginTop="56dp"
        android:autofillHints=""
        android:background="@drawable/input_bg"
        android:drawableStart="@drawable/ic_baseline_person_24"
        android:drawablePadding="?attr/listPreferredItemPaddingStart"
        android:ems="10"
        android:fontFamily="@font/poppins_light"
        android:hint="@string/nama"
        android:inputType="textPersonName"
        android:paddingVertical="5dp"
        android:paddingStart="20dp"
        android:paddingTop="10dp"
        android:paddingEnd="10dp"
        android:paddingBottom="10dp"
        android:textColor="#FF9800"
        android:textColorHint="@color/orange"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sapa"
        tools:ignore="LabelFor,TouchTargetSizeCheck,TextContrastCheck" />


    <EditText
        android:id="@+id/Mail"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:autofillHints=""
        android:background="@drawable/input_bg"
        android:drawableStart="@drawable/ic_baseline_email_24"
        android:drawablePadding="?attr/listPreferredItemPaddingStart"
        android:ems="10"
        android:fontFamily="@font/poppins_light"
        android:hint="@string/e_mail"
        android:inputType="textEmailAddress"
        android:paddingStart="20dp"
        android:paddingTop="10dp"
        android:paddingEnd="10dp"
        android:paddingBottom="10dp"
        android:textColor="#5C5858"
        android:textColorHint="@color/orange"
        app:layout_constraintEnd_toEndOf="@+id/Nama"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/Nama"
        app:layout_constraintTop_toBottomOf="@+id/Nama"
        tools:ignore="LabelFor,TouchTargetSizeCheck,TextContrastCheck" />

    <EditText
        android:id="@+id/Pass"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:autofillHints=""
        android:background="@drawable/input_bg"
        android:drawableStart="@drawable/ic_baseline_security"
        android:drawablePadding="?attr/listPreferredItemPaddingStart"
        android:ems="10"
        android:fontFamily="@font/poppins_light"
        android:hint="@string/password"
        android:inputType="textPassword"
        android:paddingStart="20dp"
        android:paddingTop="10dp"
        android:paddingEnd="10dp"
        android:paddingBottom="10dp"
        android:textColor="#5C5858"
        android:textColorHint="@color/orange"
        app:layout_constraintEnd_toEndOf="@+id/Nama"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/Nama"
        app:layout_constraintTop_toBottomOf="@+id/Mail"
        tools:ignore="LabelFor,TouchTargetSizeCheck,TextContrastCheck" />

    <EditText
        android:id="@+id/Telp"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:autofillHints=""
        android:background="@drawable/input_bg"
        android:drawableStart="@drawable/ic_baseline_phone_24"
        android:drawablePadding="?attr/listPreferredItemPaddingStart"
        android:ems="10"
        android:fontFamily="@font/poppins_light"
        android:hint="@string/no_telp"
        android:inputType="textPhonetic"
        android:paddingStart="20dp"
        android:paddingTop="10dp"
        android:paddingEnd="10dp"
        android:paddingBottom="10dp"
        android:textColor="#5C5858"
        android:textColorHint="@color/orange"
        app:layout_constraintEnd_toEndOf="@+id/Nama"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/Nama"
        app:layout_constraintTop_toBottomOf="@+id/Pass"
        tools:ignore="LabelFor,TouchTargetSizeCheck,TextContrastCheck" />

    <EditText
        android:id="@+id/Alamat"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:autofillHints=""
        android:background="@drawable/input_bg"
        android:drawableStart="@drawable/ic_baseline_location_on_24"
        android:drawablePadding="?attr/listPreferredItemPaddingStart"
        android:ems="10"
        android:fontFamily="@font/poppins_light"
        android:hint="@string/alamat"
        android:inputType="textPostalAddress"
        android:minHeight="48dp"
        android:paddingStart="20dp"
        android:paddingTop="10dp"
        android:paddingEnd="10dp"
        android:paddingBottom="10dp"
        android:textColor="#5C5858"
        android:textColorHint="@color/orange"
        app:layout_constraintEnd_toEndOf="@+id/Nama"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/Nama"
        app:layout_constraintTop_toBottomOf="@+id/Telp"
        tools:ignore="LabelFor,TextContrastCheck" />

    <EditText
        android:id="@+id/Tgl"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"

        android:autofillHints=""
        android:background="@drawable/input_bg"
        android:drawableStart="@drawable/calendar"
        android:drawablePadding="?attr/listPreferredItemPaddingStart"
        android:ems="10"
        android:fontFamily="@font/poppins_light"
        android:hint="@string/tanggal_lahir"
        android:inputType="date"
        android:minHeight="48dp"
        android:paddingStart="20dp"
        android:paddingTop="10dp"
        android:paddingEnd="10dp"
        android:paddingBottom="10dp"
        android:textColor="#5C5858"
        android:textColorHint="@color/orange"
        app:layout_constraintEnd_toEndOf="@+id/Nama"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/Nama"
        app:layout_constraintTop_toBottomOf="@+id/Alamat"
        tools:ignore="LabelFor,TextContrastCheck" />

    <Button
        android:id="@+id/submit"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:background="@drawable/btn_bg"
        android:elegantTextHeight="false"
        android:elevation="1000dp"
        android:fontFamily="@font/poppins_medium"
        android:text="@string/submit"
        android:textColor="@color/white"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/Nama"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="@+id/Nama"
        app:layout_constraintTop_toBottomOf="@+id/Tgl"
        app:layout_constraintVertical_bias="0.503"
        tools:ignore="TextContrastCheck" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contentDescription="@string/todo"
        android:onClick="login"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.044"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.019"
        app:srcCompat="@drawable/backbutton_register" />

</androidx.constraintlayout.widget.ConstraintLayout>